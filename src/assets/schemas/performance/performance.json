{
  "$id": "performance.json",
  "title": "Validate the performance object",
  "type": "object",
  "properties": {
    "tenantSubdomain": {
      "type": "string"
    },
    "userID": {
      "type": "string",
      "pattern": "^$|^[0-9a-fA-F]{24}$",
      "customType": "objectId"
    },
    "chargingStationID": {
      "type": "string"
    },
    "timestamp": {
      "type": "object",
      "format": "date-time"
    },
    "host": {
      "type": "string"
    },
    "action": {
      "type": "string"
    },
    "group": {
      "type": "string"
    },
    "durationMs": {
      "type": "number"
    },
    "reqSizeKb": {
      "type": "number"
    },
    "resSizeKb": {
      "type": "number"
    },
    "httpMethod": {
      "type": "string"
    },
    "httpUrl": {
      "type": "string"
    },
    "httpResponseCode": {
      "type": "number"
    }
  },
  "additionalProperties": false
}