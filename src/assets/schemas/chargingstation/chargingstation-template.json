                  "amperage": {
                    "type": "integer"
                  },
                  "efficiency": {
                    "type": "integer"
                  },
                  "numberOfConnectedPhase": {
                    "type": "integer",
                    "enum": [1, 3]
                  },
                  "cannotChargeInParallel": {
                    "type": "boolean"
                  },
                  "sharePowerToAllConnectors": {
                    "type": "boolean"
                  },
                  "excludeFromPowerLimitation": {
