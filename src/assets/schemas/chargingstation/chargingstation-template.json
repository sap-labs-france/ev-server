          "type": "object",
          "properties": {
            "masterSlave": {
              "type": "boolean"
            },
            "maximumPower": {
              "type": "integer"
            },
            "voltage": {
              "type": "integer"
            },
            "powerLimitUnit": {
              "type": "string"
            },
            "excludeFromPowerLimitation": {
              "type": "boolean"
